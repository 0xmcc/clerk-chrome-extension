    "remark-gfm": "^4.0.1",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/marked": "^6.0.0",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
