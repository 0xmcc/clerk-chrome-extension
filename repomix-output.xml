          {[1, 2, 3].map((i) => (
            <div
              key={i}
              className={cn(
                "plasmo-h-2.5 plasmo-w-2.5 plasmo-rounded-full",
                step >= i ? "plasmo-bg-white" : "plasmo-bg-[#1a1a1a]"
