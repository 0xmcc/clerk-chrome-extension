}

const copyOutput = (body: string) => {
  if (typeof navigator !== "undefined" && navigator.clipboard && typeof document !== "undefined") {
    const tmp = document.createElement("div")
    tmp.innerHTML = body
    navigator.clipboard.writeText(tmp.innerText)
  }
}
