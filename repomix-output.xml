          <svg fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 5l7 7m0 0l-7 7m7-7H3" />
          </svg>
        </button>
      </div>
    </div>
  </div>

  <script>
    let currentStep = 1;
    let connectedSources = new Set();
    
    function nextStep() {
      document.querySelector(`[data-step="${currentStep}"]`).classList.remove('active');
      currentStep++;
      document.querySelector(`[data-step="${currentStep}"]`).classList.add('active');
    }
    
    function prevStep() {
      document.querySelector(`[data-step="${currentStep}"]`).classList.remove('active');
      currentStep--;
      document.querySelector(`[data-step="${currentStep}"]`).classList.add('active');
    }
    
    function simulateInstall() {
      const btn = document.querySelector('.extension-btn');
      btn.innerHTML = `
        <svg width="16" height="16" fill="none" stroke="currentColor" viewBox="0 0 24 24" style="animation: spin 1s linear infinite">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4z" />
        </svg>
        Installing...
      `;
      
      setTimeout(() => {
        btn.innerHTML = `
          <svg width="16" height="16" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
          </svg>
          Installed
        `;
        btn.style.background = '#22c55e';
        
        setTimeout(() => nextStep(), 800);
      }, 1500);
    }
    
    function toggleSource(el) {
      const source = el.dataset.source;
      const statusEl = el.querySelector('.source-status');
      
      if (el.classList.contains('connected')) {
        el.classList.remove('connected');
        statusEl.textContent = 'Click to connect';
        statusEl.classList.remove('connected');
        connectedSources.delete(source);
      } else {
        el.classList.add('connected');
        statusEl.textContent = 'Connected';
        statusEl.classList.add('connected');
        connectedSources.add(source);
      }
      
      document.getElementById('continueBtn').disabled = connectedSources.size === 0;
    }
    
    function startSync() {
      nextStep();
      
      const statuses = [
        { text: 'Connecting to Claude...', count: 0 },
        { text: 'Reading Claude history...', count: 156 },
        { text: 'Connecting to ChatGPT...', count: 312 },
        { text: 'Reading ChatGPT history...', count: 523 },
        { text: 'Processing conversations...', count: 687 },
        { text: 'Extracting insights...', count: 784 },
        { text: 'Almost done...', count: 834 },
      ];
      
      let i = 0;
      const interval = setInterval(() => {
        if (i >= statuses.length) {
          clearInterval(interval);
          setTimeout(finishSync, 500);
          return;
        }
        
        document.getElementById('syncStatus').textContent = statuses[i].text;
        document.getElementById('syncCount').textContent = `${statuses[i].count} conversations found`;
        document.getElementById('progressFill').style.width = `${(i + 1) / statuses.length * 100}%`;
        i++;
      }, 800);
    }
    
    function finishSync() {
      document.getElementById('onboarding').classList.add('hidden');
      document.getElementById('app').classList.add('visible');
      document.querySelector('.demo-banner').style.display = 'none';
      document.getElementById('promptInput').focus();
    }
    
    function skipToApp() {
      document.getElementById('finalCount').textContent = '847 sample conversations loaded';
      finishSync();
    }
  </script>
</body>
</html>
