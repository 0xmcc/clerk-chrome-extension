  <div class="app" id="app">
    <div class="canvas">
      <div class="ready-state">
        <h1 class="ready-greeting">You're all set</h1>
        <p class="ready-subtext" id="finalCount">847 conversations ready to explore</p>
        
        <div class="quick-actions-label">Try asking</div>
        <div class="quick-actions">
          <button class="quick-action">"What did I figure out about..."</button>
          <button class="quick-action">"Show me my decisions on..."</button>
          <button class="quick-action">"What am I stuck on?"</button>
        </div>
      </div>
